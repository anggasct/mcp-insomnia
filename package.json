{
  "name": "mcp-insomnia",
  "version": "0.4.0",
  "description": "MCP server for making HTTP requests and managing API collections",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "watch": "tsx watch src/index.ts",
    "lint": "tsc --noEmit",
    "lint:strict": "tsc --noEmit --strict",
    "lint:eslint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "typecheck": "tsc --noEmit --pretty"
  },
  "keywords": [
    "mcp",
    "insomnia",
    "api",
    "rest",
    "graphql"
  ],
  "author": "angga permana <angga@kirana.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anggasct/mcp-insomnia.git"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.13.3",
    "axios": "^1.6.0",
    "httpsnippet": "^3.0.9",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.52.0"
  },
  "bin": {
    "mcp-insomnia": "dist/index.js"
  }
}